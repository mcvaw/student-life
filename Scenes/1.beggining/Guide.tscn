[sub_resource type="GDScript" id=1]
script/source = "extends Node2D


func get_dialog_number() -> int:
	var glob = get_node(\"/root/global\")
	if glob.loaded != \"-1\":
		var fac = get_node(\"/root/prop\").get_param(\"facility\")
		if fac == \"0\":
			return 5
		if fac == \"1\":
			return 6
		if fac == \"2\":
			return 7
	get_node(\"/root/show_nf\").show_notification(
		\"Игра не загружена\"
	)
	return -1

func activate() -> void:
	get_node(\"/root/d_obj\").show_dialog(
		get_dialog_number()
	)
"

