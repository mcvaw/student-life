script/source = "extends Node2D

onready var effect = $Tween
onready var obj = $Sprite2
onready var lgt = $Light2D
onready var cam = $Camera2D
onready var audio = $AudioStreamPlayer
export(float) var time = 5
export(float) var c_range = 0.8
export(String, FILE, \"*.tscn\") var next_scene

func _process(_delta):
	cam.position += Vector2(
		rand_range(-c_range, c_range),
		rand_range(-c_range, c_range))

func _ready():
	audio.play()
	audio.seek(7)
	effect.interpolate_property(
		obj, \"position\",
		Vector2(1086, 577),
		Vector2(983, 577),
		time, Tween.TRANS_LINEAR)
	effect.interpolate_property(
		lgt, \"position\",
		Vector2(2638, 162),
		Vector2(-490, 162),
		time, Tween.TRANS_QUAD, Tween.EASE_IN_OUT)
	effect.start()
	yield(get_tree().create_timer(time + 1), \"timeout\")
	get_node(\"/root/scene_loader\").load_scene(next_scene, false, false)
"

