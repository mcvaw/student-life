[gd_scene load_steps=4 format=2]

[ext_resource path="res://Sprites/firework.tres" type="SpriteFrames" id=1]

[sub_resource type="GDScript" id=1]
script/source = "extends Node2D

var fw = load(\"res://Audio/fx/firework_explosion_fizz_002.wav\")

onready var sprite1 : AnimatedSprite = $AnimatedSprite
onready var sprite2 : AnimatedSprite = $AnimatedSprite2
onready var sprite3 : AnimatedSprite = $AnimatedSprite3
onready var fx : = $\"/root/fx\"

export var next_scene : = \"res://Scenes/1.beggining/date2.tscn\"

func _ready() -> void:
	get_node(\"AnimationPlayer\").play(\"camera\")
	sprite1.frame = 0
	sprite2.frame = 0
	sprite3.frame = 0
	
	sprite1.play()
	yield(get_tree().create_timer(0.7), \"timeout\")
	fx.play_sound(fw)
	
	yield(get_tree().create_timer(0.08), \"timeout\")
	sprite2.play()
	yield(get_tree().create_timer(0.7), \"timeout\")
	fx.play_sound(fw)
	
	yield(get_tree().create_timer(0.4), \"timeout\")
	sprite3.play()
	yield(get_tree().create_timer(0.7), \"timeout\")
	fx.play_sound(fw)


func _on_AnimatedSprite_animation_finished() -> void:
	assert(get_tree().change_scene(next_scene) == OK)
"

