[sub_resource type="GDScript" id=1]
script/source = "extends Node2D


var fw = load(\"res://Audio/fx/firework_explosion_fizz_002.wav\")

onready var fx : = $\"/root/fx\"

export (String, FILE, \"*.tscn\") var next_scene


func _ready() -> void:
	yield(get_tree().create_timer(0.7), \"timeout\")
	get_node(\"AnimationPlayer\").play(\"camera\")
	
	yield(get_tree().create_timer(0.7), \"timeout\")
	fx.play_sound(fw)
	
	yield(get_tree().create_timer(2), \"timeout\")
	fx.play_sound(fw)


func _on_AnimationPlayer_animation_finished(_anim_name) -> void:
	get_node(\"/root/scene_loader\").load_scene(next_scene)
"

