[sub_resource type="GDScript" id=4]
script/source = "extends Sprite

var posx = 800
var speed = 250
const delay = 18
var cf = 0

export (String, FILE, \"*.json\") var dialog_path


func _ready() -> void:
	set_process(false)


func blood() -> void:
	position.x += 10
	$AnimatedSprite.visible = true
	$AnimatedSprite.play()


func show_singer() -> void:
	get_node(\"../Singer\").enable_ai()


func enable_ai() -> void:
	set_process(true)
	visible = true
	

func _process(delta):
	if position.x < posx:
		position.x += delta * speed
		if cf < delay:
			cf += 1
		else:
			cf = 0
			if frame == 2:
				frame = 5
			else:
				frame = 2
	else:
		frame = 8
		set_process(false)
		DIALOG.show_dialog(dialog_path, get_path(), \"show_singer\")
"

[sub_resource type="GDScript" id=5]
script/source = "extends Sprite

var posx = 600
var speed = 250
const delay = 13
var cf = 0

export (String, FILE, \"*.json\") var dialog_path
export (String, FILE, \"*.tscn\") var scene_path


func _ready() -> void:
	set_process(false)
	

func enable_ai() -> void:
	set_process(true)
	visible = true
	

func _process(delta):
	if position.x < posx:
		position.x += delta * speed
		if cf < delay:
			cf += 1
		else:
			cf = 0
			if frame == 0:
				frame = 1
			else:
				frame = 0
	else:
		frame = 8
		set_process(false)
		get_node(\"../Neo\").flip_h = true
		DIALOG.show_dialog(dialog_path, get_path(), \"shot\")


func shot() -> void:
	yield(get_tree().create_timer(2), \"timeout\")
	frame = 2
	yield(get_tree().create_timer(1), \"timeout\")
	get_node(\"../Neo\").blood()
	yield(get_tree().create_timer(1.3), \"timeout\")
	SCENES.load_scene(scene_path, false, false)
"

